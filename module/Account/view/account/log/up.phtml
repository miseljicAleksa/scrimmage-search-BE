     /.social-auth-links -->

    <!--<a href="#">I forgot my password</a><br> -->
    <a href="/account/log/in" class="text-center">Sign In</a>

  </div>
  <!-- /.login-box-body -->
</div>
<!-- /.login-box -->
<script>
  $(function () {
    $('input').iCheck({
      checkboxClass: 'icheckbox_square-blue',
      radioClass: 'iradio_square-blue',
      increaseArea: '20%' /* optional */
    });
  });
</script>